{
  "manifest_version": 3,
  "name": "ChatGPT Scroller",
  "description": "Navigate ChatGPT conversations more easily with navigation buttons.",
  "version": "0.2.0",
  "icons": {
    "16": "src/images/icons/icon16.png",
    "32": "src/images/icons/icon32.png",
    "48": "src/images/icons/icon48.png",
    "128": "src/images/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["dist/content.js"],
      "css": ["styles.css"]
    }
  ]
}
